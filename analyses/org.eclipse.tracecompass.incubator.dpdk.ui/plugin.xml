<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.linuxtools.tmf.core.analysis">
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput">
         <analysisId
               id="org.eclipse.tracecompass.incubator.dpdk.vhost.analysis">
         </analysisId>
      </output>
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput">
         <analysisId
               id="org.eclipse.tracecompass.incubator.dpdk.lpm.analysis">
         </analysisId>
      </output>
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput">
         <analysisId
               id="org.eclipse.tracecompass.incubator.dpdk.lookup.objects.analysis">
         </analysisId>
      </output>
      <output
            class="org.eclipse.tracecompass.tmf.ui.analysis.TmfAnalysisViewOutput">
         <analysisId
               id="org.eclipse.tracecompass.incubator.dpdk.classifier.analysis">
         </analysisId>
      </output>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.category.dpdk" 
            name="%category.dpdk">
      </category>
      <view
            category="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.category.dpdk"
            class="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.DpdkVhostUserAvailDescrView"
            icon="icons/eview16/scatter.png"
            id="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.avail.descr.view"
            name="Vhost Ring Queues Status"
            restorable="true">
      </view>
      <view
            category="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.category.dpdk"
            class="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.packet.rate.DpdkVhostUserPacketRateView"
            icon="icons/eview16/scatter.png"
            id="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.packet.rate.view"
            name="Vhost Packet rate"
            restorable="true">
      </view>
      <view
            category="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.category.dpdk"
            class="org.eclipse.tracecompass.incubator.internal.dpdk.ui.lookup.object.hit.percentage.DpdkLookupObjectHitPercentageView"
            icon="icons/eview16/scatter.png"
            id="org.eclipse.tracecompass.incubator.internal.dpdk.ui.lookup.object.hit.percentage.view"
            name="LPM Hit/Miss Percentage"
            restorable="true">
      </view>
      <view
            category="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.category.dpdk"
            class="org.eclipse.tracecompass.incubator.internal.dpdk.ui.lpm.rule.hit.rate.DpdkLpmRuleHitRateView"
            icon="icons/eview16/scatter.png"
            id="org.eclipse.tracecompass.incubator.internal.dpdk.ui.lpm.rule.hit.rate.view"
            name="Per-rule lpm lookup hit"
            restorable="true">
      </view>
      <view
            category="org.eclipse.tracecompass.incubator.internal.dpdk.ui.vhost.category.dpdk"
            class="org.eclipse.tracecompass.incubator.internal.dpdk.ui.flow.classifier.lookup.rate.DpdkFlowClassifierLookupHitRateView"
            icon="icons/eview16/scatter.png"
            id="org.eclipse.tracecompass.incubator.internal.dpdk.ui.flow.classifier.lookup.rate.view"
            name="Flow Classifier Lookup Rate"
            restorable="true">
      </view>
   </extension>

</plugin>
